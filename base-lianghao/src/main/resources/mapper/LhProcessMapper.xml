<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.joolun.cloud.lianghao.mapper.LhProcessMapper">

  <resultMap id="lhProcessMap" type="com.joolun.cloud.lianghao.entity.LhProcess">
		<id property="id" column="id"/>
		<result property="name" column="name"/>
		<result property="operator" column="operator"/>
		<result property="description" column="description"/>
  </resultMap>
	<select id="selectProcess" resultType="java.util.HashMap">
    	select CONCAT(id,'-',name) as value,id,name as label from lh_process
    </select>
</mapper>
